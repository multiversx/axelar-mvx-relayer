    "@multiversx/sdk-nestjs-cache": "^3.7.0",
    "@multiversx/sdk-nestjs-common": "^3.7.0",
    "@multiversx/sdk-nestjs-elastic": "^3.7.0",
    "@multiversx/sdk-nestjs-http": "^3.7.0",
    "@multiversx/sdk-nestjs-monitoring": "^3.7.0",
    "@multiversx/sdk-nestjs-rabbitmq": "^3.7.0",
    "@multiversx/sdk-nestjs-redis": "^3.7.0",
    "@multiversx/sdk-network-providers": "^2.4.3",
    "@multiversx/sdk-transaction-processor": "^0.1.33",
    "@multiversx/sdk-wallet": "^4.5.0",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/schedule": "^4.0.2",
    "@prisma/client": "^5.15.0",
    "agentkeepalive": "^4.5.0",
    "bignumber.js": "9.0.1",
    "bull": "^4.12.9",
    "cache-manager": "^5.6.1",
    "cron": "^3.1.7",
    "ethers": "^6.13.0",
    "ioredis": "^5.4.1",
    "module-alias": "^2.2.3",
    "nest-winston": "^1.10.0",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.5.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/cache-manager": "^4.0.6",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.2",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.3.2",
    "prisma": "^5.15.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.178.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "_moduleAliases": {
    "@mvx-monorepo/common": "dist/libs/common"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@mvx-monorepo/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@mvx-monorepo/common": "<rootDir>/libs/common"
    }
  }
}
